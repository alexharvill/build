[MASTER]
extension-pkg-whitelist=cv2,nanogui,aruco,tensorflow
unsafe-load-any-extension=yes

[TYPECHECK]
ignored-modules=boto3,botocore,scipy.spatial.qhull

[MESSAGES CONTROL]
disable=W0311,C0103,C0330,useless-object-inheritance

[REPORTS]
output-format=text
msg-template={line},{column},{category},{symbol}:{msg}
